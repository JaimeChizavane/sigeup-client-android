<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mz.ac.sigeup.sigeup_navigationview">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="mz.ac.sigeup.sigeup_navigationview.app.AppController"
        android:allowBackup="true"
        android:icon="@drawable/up"
        android:label="@string/app_name"
        android:roundIcon="@drawable/up"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="mz.ac.sigeup.sigeup_navigationview.activity.LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="mz.ac.sigeup.sigeup_navigationview.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>

    </application>

</manifest>